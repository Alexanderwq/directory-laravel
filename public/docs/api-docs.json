        },
        "/api/buildings/{id}": {
            "get": {
                "tags": [
                    "Buildings"
                ],
                "summary": "Показать здание",
                "operationId": "7c2e9a5ec44b127b681717d1d5b91152",
                "parameters": [
                    {
                        "name": "id",
